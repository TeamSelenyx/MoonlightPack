                        { "servers@start.servers_dynamic_start_button": { "size": [ "100%", "50%" ] } }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "button_stack_2@common.horizontal_stack_panel": {
                "size": [ "100%", "45%" ],
                "anchor_to": "center",
                "anchor_from": "center",
                "controls": [
                  { "setting@start.setting_dynamic_start_button": { "size": [ "30%", "100%" ] } },
                  {
                    "button_stack_sign@common.horizontal_stack_panel": {
                      "size": [ "70%", "100%" ],
                      "anchor_to": "center",
                      "anchor_from": "center",
                      "controls": [
                        {
                          "button_stack_1@common.vertical_stack_panel": {
                            "size": [ "50%", "100%" ],
                            "anchor_to": "center",
                            "anchor_from": "center",
                            "controls": [
                              {
                                "dressing_room@start.dressing_room_dynamic_start_button": { "size": [ "100%", "50%" ] }
                              },
                              {
                                "marketplace@start.marketplace_dynamic_start_button": { "size": [ "100%", "50%" ] }
                              }
                            ]
                          }
                        },
                        {
                          "button_stack_2@common.vertical_stack_panel": {
                            "size": [ "50%", "100%" ],
                            "anchor_to": "center",
