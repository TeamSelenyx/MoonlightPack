                                  "size": [ "100%", "50%" ],
                                  "bindings": [
                                    { "binding_name": "(not #sign_in_visible)", "binding_name_override": "#visible" }
                                  ]
                                }
                              },
                              {
                                "login@start.login_dynamic_start_button": {
                                  "size": [ "100%", "100%" ],
