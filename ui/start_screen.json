  },

  "dressing_room_dynamic_icon@start.dynamic_icon_base": {
    "texture": "textures/ui/start/icon_dressing_room"
  },

  "marketplace_dynamic_icon@start.dynamic_icon_base": {
    "texture": "textures/ui/sidebar_icons/marketplace"
  },

  "settings_dynamic_icon@start.dynamic_icon_base": {
