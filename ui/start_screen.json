                        }
                      ]
                    }
                  },
                  {
                    "border_black@common.focus_border_black": {
                      "layer": 3,
                      "size": [ "100%sm", "100%sm" ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "servers_dynamic_icon@start.dynamic_icon_base": {
    "texture": "textures/ui/servers"
  },

  "none_dynamic_icon@start.dynamic_icon_base": {
    "texture": "textures/ui/icon_none"
  },


  "dynamic_start_button_base": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$button_mappings|default": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "$pressed_button_name",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "$pressed_button_name",
        "mapping_type": "pressed"
      }
    ],
    "controls": [
      {
        "button_cover_image": {
          "type": "image",
          "size": [ "100% - 4px", "100% - 4px" ],
          "texture": "textures/ui/border_line",
          "$border_texture_color_1|default": [ 1.0, 1.0, 1.0 ],
          "$border_texture_color_2|default": [ 1.0, 1.0, 1.0 ],
          "color": "$border_texture_color_1",
          "controls": [
            {
              "button@button_preset.dynamic_start_button_base": {
                "size": [ "100% - 2px", "100% - 2px" ],
                "anchor_from": "center",
                "anchor_to": "center",
                "button_mappings": "$button_mappings"
              }
            }
          ]
        }
      }
    ]
  },

  "login_dynamic_start_button@start.dynamic_start_button_base": {
    "$border_texture_color_1": [ 0.776, 0.776, 0.776 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.login_icon",
    "$pressed_button_name": "gui.signIn",
    "$button_background_image": "textures/ui/sunset_keyart",
    "$button_text": "xbox.signin"
  },

  "play_dynamic_start_button@start.dynamic_start_button_base": {
    "$border_texture_color_1": [ 0.949, 0.949, 0.949 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.play_dynamic_icon",
    "$pressed_button_name": "$play_button_target",
    "$button_background_image": "textures/ui/start/start_background_play",
    "$button_text": "menu.play"
  },

  "get_started_dynamic_start_button@start.dynamic_start_button_base": {
    "$border_texture_color_1": [ 0.949, 0.949, 0.949 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.get_started_dynamic_icon",
    "$pressed_button_name": "button.menu_get_started",
    "$button_background_image": "textures/ui/start/start_background_get_started",
    "$button_text": "menu.getStarted"
  },

  "servers_dynamic_start_button@start.dynamic_start_button_base": {
    "$border_texture_color_1": [ 0.251, 0.878, 0.816 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.servers_dynamic_icon",
    "$pressed_button_name": "button.menu_servers",
    "$button_background_image": "textures/ui/start/start_background_servers",
    "$button_text": "menu.servers"
  },

  "setting_dynamic_start_button@start.dynamic_start_button_base": {
    "$border_texture_color_1": [ 0.749, 0.725, 0.714 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.settings_dynamic_icon",
    "$pressed_button_name": "button.menu_settings",
    "$button_background_image": "textures/ui/start/start_background_settings",
    "$button_text": "menu.settings"
  },

  "marketplace_dynamic_start_button@start.dynamic_start_button_base": {
    "$button_type_panel": "button_content_preset.horizontal_dynamic_upgrade_button_content",

    "$border_texture_color_1": [ 0.69, 0.769, 0.871 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.marketplace_dynamic_icon",
    "$pressed_button_name": "button.menu_store",
    "$button_background_image": "textures/ui/start/start_background_marketplace",
    "$button_text": "menu.store"
  },

  "inbox_dynamic_start_button@start.dynamic_start_button_base": {
    "$button_type_panel": "button_content_preset.horizontal_dynamic_upgrade_button_content",

    "$border_texture_color_1": [ 0.827, 0.827, 0.827 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.inbox_bell_animated_dynamic_icon",
    "$pressed_button_name": "button.menu_inbox",
    "$button_background_image": "textures/ui/start/start_background_inbox",
    "$button_text": "menu.inbox",
    "$button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "$pressed_button_name",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "$pressed_button_name",
        "mapping_type": "pressed"
      },
      {
        "to_button_id": "inbox_button.is_hovered",
        "mapping_type": "pressed"
      }
    ]
  },

  "dressing_room_dynamic_start_button@start.dynamic_start_button_base": {
    "$button_type_panel": "button_content_preset.horizontal_dynamic_upgrade_button_content",

    "$border_texture_color_1": [ 0.459, 0.624, 0.835 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.dressing_room_dynamic_icon",
    "$pressed_button_name": "button.to_profile_screen",
    "$button_background_image": "textures/ui/start/start_background_dressing_room",
    "$button_text": "profileScreen.header",
    "bindings": [
      {
        "binding_name": "#dressing_room_button_visible",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "achievements_dynamic_start_button@start.dynamic_start_button_base": {
    "$button_type_panel": "button_content_preset.horizontal_dynamic_upgrade_button_content",

    "$border_texture_color_1": [ 0.792, 1, 0.976 ],
    "$border_texture_color_2": [ 1.0, 1.0, 1.0 ],
    "$button_icon_content": "start.achievements_dynamic_icon",
    "$pressed_button_name": "button.menu_achievements",
    "$button_background_image": "textures/ui/start/start_background_achievements",
    "$button_text": "menu.achievements"
  },

  "none_dynamic_start_button@start.dynamic_start_button_base": {
    "enabled": false,
    "$button_type_panel": "button_content_preset.horizontal_dynamic_upgrade_button_content",

    "$button_icon_content": "start.none_dynamic_icon",
    "$pressed_button_name": "button.none",
    "$button_background_image": "textures/ui/LoadingWorldDemoScreen",
    "$button_text": "none"
  },


  //---------------------------------------------------------------------------
  // Start Screen: Sub Panel
  //---------------------------------------------------------------------------
  "skin_viewer_panel": {
    "type": "input_panel",
    "anchor_from": "center",
    "anchor_to": "center",
    "size": [ "100%", "100%" ],
    "offset": [ 0, 0 ],
    "gesture_tracking_button": "button.turn_doll",
    "tts_ignore_count": true,
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.turn_doll",
        "mapping_type": "pressed",
        "button_up_right_of_first_refusal": true
      }
    ],
    "controls": [
      {
        "paper_doll_panel": {
          "type": "panel",
          "size": [ "100%", "100%" ],
          "controls": [
            {
              "paper_doll": {
                "layer": 2,
                "type": "custom",
                "size": [ "100%", "100%" ],
                "offset": [ 0, "-16%" ],
                "renderer": "paper_doll_renderer",
                "camera_tilt_degrees": -10,
                "starting_rotation": -20,
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "rotation": "gesture_x",

                "animation_reset_name": "screen_animation_reset",
                "anims": [
                  "@common.screen_exit_size_animation_push",
                  "@common.screen_exit_size_animation_pop",
                  "@common.screen_entrance_size_animation_push",
                  "@common.screen_entrance_size_animation_pop"
                ],

                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_delta_source",
                    "target_property_name": "#gesture_delta_source"
                  },
                  {
                    "binding_type": "view",
                    "source_control_name": "viewer_panel",
                    "source_property_name": "#gesture_mouse_delta_x",
                    "target_property_name": "#gesture_mouse_delta_x"
                  },
                  {
                    "binding_name": "#is_appearance_visible",
                    "binding_name_override": "#visible",
                    "binding_condition": "always"
                  }
                ]
              }
            },
            {
              "appearacne_loading_panel@common_store.progress_loading": {
                "$hide_failure_text": true,
                "$hide_progress_loading_outline": true,
                "$source_control_name": "paper_doll",
                "$source_property_name": "(not #visible)"
              }
            }
          ]
        }
      },
      {
        "paper_doll_name_tag": {
          "layer": 2,
          "visible": false,
          "type": "custom",
          "renderer": "name_tag_renderer",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "size": [ 8, 8 ],
          "bindings": [
            {
              "binding_name": "#playername"
            }
          ]
        }
      }
    ]
  },

  "skin_panel": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "offset": [ 0, 0 ],
    "anchor_to": "center",
    "anchor_from": "center",
    "controls": [
      { "viewer_panel@start.skin_viewer_panel": {} }
    ]
  },


  // Top Content Panel
  "top_content_panel_gradient": {
    "type": "custom",
    "size": [ "100%", "100%" ],
    "renderer": "gradient_renderer",
    "gradient_direction": "horizontal",
    "color1": [ 1.0, 1.0, 1.0, 0.0 ],
    "color2": [ 1.0, 1.0, 1.0, 0.7 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle"
  },

  "top_content_panel": {
    "type": "panel",
    "size": [ "200%", 32 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      { "background_image@start.top_content_panel_gradient": { "size": [ "27%", "100%" ], "offset": [ "-15%", 0 ],  "anchor_from": "right_middle", "anchor_to": "right_middle" } },
      {
        "game_text_panel": {
          "type": "panel",
          "size": [ "50% - 10px", "100% - 4px" ],
          "anchor_from": "center",
          "anchor_to": "center",
          "controls": [
            {
              "player_info_panel": {
                "type": "panel",
                "size": [ "100%cm", "100%" ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "controls": [
                  { "player_info@start.player_info_panel": {} },
                  {
                    "profile_button@common_buttons.light_content_button": {
                      "size": [ "100%sm", "100%" ],
                      "offset": [ -4, 0 ],

                      "$pressed_button_name": "button.menu_profile",

                      "$default_state_border_visible": false,
                      "$locked_state_border_visible": false,

                      "$default_button_texture": "textures/ui/empty",
                      "$hover_button_texture": "textures/ui/empty",
                      "$pressed_button_texture": "textures/ui/empty",
                      "$locked_button_texture": "textures/ui/empty"
                    }
                  }
                ],
                "bindings": [
                  {
                    "binding_name": "(not #sign_in_visible)",
                    "binding_name_override": "#visible"
                  }
                ]
              }
            },
            {
              "game_info_panel": {
                "type": "panel",
                "size": [ "100%", "100%" ],
                "offset": [ 0, 4 ],
                "anchor_from": "center",
                "anchor_to": "center",
                "controls": [
                  { "game_info@start.game_info_panel": { "anchor_from": "right_middle", "anchor_to": "right_middle" } }
                ]
              }
            }
          ]
        }
      }
    ]
  },


  // Game Info
  "game_info_panel": {
    "type": "stack_panel",
    "size": [ "100%", "100%cm" ],
    "controls": [
      {
        "copyright_panel": {
          "type": "panel",
          "size": [ "100%", "100%c" ],
          "controls": [
            {
              "copyright@start.game_info_label_base": {
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "$info_text": "menu.copyright",
                "$info_text_bindings": []
              }
            }
          ]
        }
      },
      {
        "game_version_panel": {
          "type": "panel",
          "size": [ "100%", "100%s" ],
          "controls": [
            {
              "game_version@start.game_version_info": { "offset": [ 2, 0 ], "anchor_from": "right_middle", "anchor_to": "right_middle" }
            }
          ]
        }
      }
    ]
  },

  "game_version_info": {
    "type": "stack_panel",
    "size": [ "100%c", "100%s" ],
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "orientation": "horizontal",
    "controls": [
      {
        "version_label@start.game_info_label_base": { "$info_text": "#version" }
      },
      { "start_text": { "type": "label", "size": [ "default", "default" ], "text": "[ ", "shadow": true } },
      {
        "development_version_label@start.game_info_label_base": { "$info_text": "#development_version" }
      },
      { "end_text": { "type": "label", "size": [ "default", "default" ], "text": " ]", "shadow": true } }
    ]
  },

  "game_info_label_base": {
    "$info_text|default": "",
    "$info_text_bindings|default": [
      { "binding_name": "$info_text" }
    ],

    "layer": 2,
    "type": "label",
    "size": [ "default", "default" ],
    "max_size": [ "default", "default" ],
    "text": "$info_text",
    "text_alignment": "center",
    "color": "$main_header_text_color",
    "shadow": true,
    "bindings": "$info_text_bindings"
  },


  // Player Info
  "player_info_panel": {
    "type": "panel",
    "size": [ "100%c + 6px", "100%" ],

    "animation_reset_name": "screen_animation_reset",
    "anims": [
      "@common.screen_exit_animation_push_offset",
      "@common.screen_exit_animation_pop_offset",
      "@common.screen_entrance_animation_push_offset",
      "@common.screen_entrance_animation_pop_offset"
    ],

    "controls": [
      { "gamertag@start.player_info_content_panel": {} }
    ]
  },

  "player_info_content_panel": {
    "type": "panel",
    "size": [ "100%c", "30px" ],
    "offset": [ 0, 0 ],
    "anchor_from": "center",
    "anchor_to": "center",
    "controls": [
      {
        "player_info_content": {
          "type": "stack_panel",
          "size": [ "100%c", 26 ],
          "offset": [ -3, 0 ],
          "anchor_from": "center",
          "anchor_to": "center",
          "orientation": "horizontal",
          "controls": [
            {
              "gamerpic_with_border": {
                "type": "panel",
                "size": [ "100%cm", "100%" ],
                "controls": [
                  { "gamerpic@start.player_profile_image": { "layer": 1, "size": [ 26, 26 ] } },
                  { "border_black@common.focus_border_black": { "layer": 3, "size": [ 26, 26 ] } }
                ]
              }
            },
            { "spacer@common.empty_panel": { "size": [ 2, 0 ] } },
            {
              "gamertag_label_panel": {
                "type": "panel",
                "size": [ "100%cm + 8px", "100%" ],
                "offset": [ 0, 0 ],
                "$gamertag_offset|default": [ 0, 0 ],
                "$gamertag_color|default": [ 1.0, 1.0, 1.0 ],
                "controls": [
                  { "gamertag_lightBlue@start.player_gamertag_label": { "$gamertag_layer": 1, "$gamertag_color": [ 0.0, 1.0, 1.0 ], "$gamertag_offset": [ -1, -2 ] } },
                  { "gamertag_white@start.player_gamertag_label": { "$gamertag_layer": 2, "$gamertag_offset": [ 0, -1 ] } },
                  { "gamertag_pink@start.player_gamertag_label": { "$gamertag_layer": 1, "$gamertag_color": [ 1.0, 0.0, 1.0 ], "$gamertag_offset": [ 1, 0 ] } }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "player_profile_image": {
    "layer": 2,
    "type": "custom",
    "size": [ "100%", "100%" ],
    "renderer": "profile_image_renderer",
    "bindings": [
      {
        "binding_name": "#profile_image_options"
      }
    ]
  },

  "player_gamertag_label": { // Xbox Live
    "$gamertag_layer|default": 1,
    "layer": "$gamertag_layer",
    "type": "label",
    "size": [ "default", "default" ],
    "offset": "$gamertag_offset",
    "anchor_from": "center",
    "anchor_to": "center",
    "text": "#gamertag_label",
    "color": "$gamertag_color",
    "locked_alpha": 0.5,
    "font_type": "MinecraftTen",
    "font_scale_factor": 1.4,
    "bindings": [
      { "binding_name": "#gamertag_label" }
    ]
  },

  "player_name_label@start.player_gamertag_label": { // Local
    "text": "#playername",
    "bindings": [
      { "binding_name": "#playername" }
    ]
  },


  // Dynamic Buttons
