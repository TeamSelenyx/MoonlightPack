    "controls": [
      {
        "content_stack_panel@common.vertical_stack_panel": {
          "size": [ "100%", "100%" ],
          "anchor_to": "center",
          "anchor_from": "center",
          "controls": [
            {
              "button_stack_1@common.horizontal_stack_panel": {
                "size": [ "100%", "55%" ],
                "anchor_to": "center",
                "anchor_from": "center",
