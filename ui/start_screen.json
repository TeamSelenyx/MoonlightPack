  },

  "realms_dynamic_icon@start.dynamic_icon_base": {
    "texture": "textures/ui/realmsIcon"
  },

  "profile_dynamic_icon": {
    "type": "panel",
    "size": [ 16, 16 ],
    "controls": [
      {
        "gamerpic_offset_wrapper": {
          "type": "panel",
          "size": [ "100%", "100%" ],
          "controls": [
            {
              "gamerpic_with_border": {
                "type": "panel",
                "size": [ "100%", "100%" ],
                "offset": [ -2, 0 ],
                "controls": [
                  {
                    "gamerpic@start.gamerpic": {
                      "$gamerpic_visible": "#show_gamerpic"
